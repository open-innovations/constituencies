{
	"title": "Tram and metro stops",
	"config": {
		"width": 600,
		"hexjson": "hexjson.uk-constituencies-2024",
		"data": "release.all",
		"columns": [{
			"name": "filterLabel",
			"template": "{{ n }} ({{ MET_density }})"
		}],
		"value": "MET_density",
