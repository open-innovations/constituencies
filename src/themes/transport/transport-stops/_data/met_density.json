			"position": "top right",
			"items": [{
				"value": 0.75,
				"label": "0.75 / square km"
			},{
				"value": 0.5,
				"label": "0.5 / square km"
			},{
				"value": 0.25,
				"label": "0.25 / square km"
			}]
		},
		"tools": {
			"filter": {
				"label": "filterLabel"
			}
		},
		"tooltip": "<strong>{{ n }}</strong><br />Stops / square km: <strong>{{ MET_density | toLocaleString }}</strong>",
