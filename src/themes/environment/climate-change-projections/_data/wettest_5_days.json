{
	"title": "Climate change: wettest 5 days projections (UK)",
	"config": {
		"width": 600,
		"hexjson": "hexjson.uk-constituencies-2024",
		"data": "release.climate_projections_oacd-ucl",
		"columns": [{
			"name": "filterLabel",
			"template": "{{ n }}"
		}],
		"min": -10,
		"max": 10,
		"value": "rx5day_proj",
		"scale": "#ffffcc 0%, #a1dab4 25%, #41b6c4 50%, #2c7fb8 75%, #253494 100%",
		"matchKey": "constituency",
		"legend": {
			"position": "top right",
			"items": [{
				"value": 10,
				"label": "+10%"
			},{
				"value": 5,
				"label": "+5%"
			},{
				"value": 0,
				"label": "0.0%"
			},{
				"value": -5,
				"label": "-5%"
			},{
				"value": -10,
				"label": "-10%"
			}]
		},
		"tools": {
			"filter": {
				"label": "filterLabel"
			}
		},
		"tooltip": "<strong>{{ n }}</strong><br />Change in wettest 5 days: <strong>{{ rx5day_proj | toFixed(1) }}%</strong>",
		"attribution": "<a href=\"https://github-pages.ucl.ac.uk/UK-climate-by-mp/?var=rx5day&period=proj\">Climate change projections</a> (UCL/Copernicus/EuroCORDEX)"
	},
	"date": "2025",
	"units": {
		"prdjf_proj":{"value":"%","notes":"Change for 2021-2040 compared to 1981-2010"},
		"prjja_proj":{"value":"%","notes":"Change for 2021-2040 compared to 1981-2010"},
		"rx5day_proj":{"value":"%","notes":"Change for 2021-2040 compared to 1981-2010"},
		"txx_proj":{"value":"degC","notes":"Change for 2021-2040 compared to 1981-2010"},
		"t_proj":{"value":"degC","notes":"Change for 2021-2040 compared to 1981-2010"}
	}
}