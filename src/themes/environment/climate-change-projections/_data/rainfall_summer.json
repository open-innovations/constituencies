{
	"title": "Climate change: summer rainfall projections (UK)",
	"config": {
		"width": 600,
		"hexjson": "hexjson.uk-constituencies-2024",
		"data": "release.climate_projections_oacd-ucl",
		"columns": [{
			"name": "filterLabel",
			"template": "{{ n }}"
		}],
		"min": -10,
		"max": 10,
		"value": "prjja_proj",
		"scale": "#543005 0%, #8c510a 10%, #bf812d 20%, #dfc27d 30%, #f6e8c3 40%, #f5f5f5 50%, #c7eae5 60%, #80cdc1 70%, #35978f 80%, #01665e 90%, #003c30 100%",
		"matchKey": "constituency",
		"legend": {
			"position": "top right",
			"items": [{
				"value": 10,
				"label": "+10%"
			},{
				"value": 5,
				"label": "+5%"
			},{
				"value": 0,
				"label": "0.0%"
			},{
				"value": -5,
				"label": "-5%"
			},{
				"value": -10,
				"label": "-10%"
			}]
		},
		"tools": {
			"filter": {
				"label": "filterLabel"
			}
		},
		"tooltip": "<strong>{{ n }}</strong><br />Change in summer precipitation: <strong>{{ prjja_proj | toFixed(1) }}%</strong>",
		"attribution": "<a href=\"https://github-pages.ucl.ac.uk/UK-climate-by-mp/?var=prjja&period=proj\">Climate change projections</a> (UCL/Copernicus/EuroCORDEX)"
	},
	"date": "2025",
	"units": {
		"prdjf_proj":{"value":"percent","notes":"Change for 2021-2040 compared to 1981-2010"},
		"prjja_proj":{"value":"percent","notes":"Change for 2021-2040 compared to 1981-2010"},
		"rx5day_proj":{"value":"percent","notes":"Change for 2021-2040 compared to 1981-2010"},
		"txx_proj":{"value":"degC","notes":"Change for 2021-2040 compared to 1981-2010"},
		"t_proj":{"value":"degC","notes":"Change for 2021-2040 compared to 1981-2010"}
	}
}