{
	"title": "Ancient woodland",
	"config": {
		"width": 600,
		"hexjson": "hexjson.uk-constituencies-2024",
		"data": "sources.environment.ancient-woodland",
		"columns":[{
			"name": "filterLabel",
			"template": "{{ n }}"
		}],
		"value": "Percentage",
		"scale": "#dfdfdf 0%, #0DBC37 100%",
		"min": 0,
		"max": 20,
		"matchKey": "Code",
		"legend":{
			"position": "top right",
			"items": [{
				"value": 20,
				"label": "20%"
			},{
				"value": 15,
				"label": "15%"
			},{
				"value": 10,
				"label": "10%"
			},{
				"value": 5,
				"label": "5%"
			},{
				"value": 0,
				"label": "0%"
			}]
		},
		"tools":{
			"filter":{
				"label": "filterLabel"
			}
		},
		"tooltip": "<strong>{{ n }}</strong><br />{{ Area | multiply(0.0001) | toFixed(0) | toLocaleString() }} hectares ({{ Percentage | toFixed(1) | toLocaleString() }}%)",
		"attribution": "Data: <a href=\"https://www.planning.data.gov.uk/dataset/ancient-woodland\">Ancient woodland</a>, Natural England and contains Ordnance Survey data Â© Crown copyright and database right 2024 (OGLv3)"
	}
}