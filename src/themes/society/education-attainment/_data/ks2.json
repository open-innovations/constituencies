{
	"title": "Educational attainment: KS2 Attainment",
	"config": {
		"width": 600,
		"hexjson": "hexjson.constituencies",
		"data": "sources.society.education-attainment",
		"columns": [{
			"name": "filterLabel",
			"template": "{{ n }} ({{ a }})"
		}],
