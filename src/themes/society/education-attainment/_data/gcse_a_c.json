		"A*-C % / 2017":{"value":"percent"},
		"A*-C % / 2018":{"value":"percent"},
		"A*-C % / 2019":{"value":"percent"},
		"A*-C % / 2020":{"value":"percent"},
		"A*-C % / 2021":{"value":"percent"},
		"A*-C % / 2022":{"value":"percent"}
